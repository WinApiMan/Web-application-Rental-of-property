@model int

<script src="/js/_AdsPage.js"></script>
@{ 
    const int Success = 1;

    var resultText = JsonSerializer.Serialize(Localizer["SuccessfulLoad"].Value);

    if (Model == Success)
    {
    <script>
        let resultText =JSON.parse(JSON.stringify(@Html.Raw(resultText)));
        outputLoadResult(resultText);
    </script>
    }
}